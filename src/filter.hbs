<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Assignment</th>
            <th>Acronym</th>
            <th>S89</th>
        </tr>
    </thead>
    <tbody>
        {{#each subtitles}}
        <tr>
            <th style="text-align: center;">
                <input class="form-check-input" type="checkbox" id="{{@key}}" value="{{@key}}" checked />
            </th>
            <td>{{ this.label }}</td>
            <td>{{ @key }}</td>
            <td>{{#if this.S89 }}<i class="fa fa-check"></i>{{/if}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
