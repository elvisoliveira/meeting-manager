{
    "extends": "@parcel/config-default",
    "resolvers": ["@parcel/resolver-glob", "..."],
    "transformers": {
        "*.{handlebars,hbs}": ["@parcel/transformer-raw"]
    },
    "compressors": {
        "*.{html,css,js,hbs}": [
            "...",
            "@parcel/compressor-gzip",
            "@parcel/compressor-brotli"
        ]
    }
}
